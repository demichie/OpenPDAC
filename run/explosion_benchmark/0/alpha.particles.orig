FoamFile
{
    format      binary;
    class       volScalarField;
    location    "0";
    object      alpha.particles;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField  uniform 0.0;

boundaryField
{
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }
    sides
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }
    terrain
    {
        type            zeroGradient;
    }
    conduitwalls
    {
        type            zeroGradient;
    }
    conduitbottom
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}


