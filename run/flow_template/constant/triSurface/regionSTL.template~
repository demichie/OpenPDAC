solid groundAndWall
 facet normal 0 1 0
  outer loop
   vertex xmax zground 0
   vertex 0 zground 0
   vertex 0 zground 300
  endloop
 endfacet
 facet normal 0 1 -0
  outer loop
   vertex xmax zground 0
   vertex 0 zground 300
   vertex xmax zground 300
  endloop
 endfacet
endsolid groundAndWall
solid nozzel
 facet normal 0 -1 0
  outer loop
   vertex -r0 h0 0
   vertex r0 h0 0
   vertex r0 h0 300
  endloop
 endfacet
 facet normal 0 -1 0
  outer loop
   vertex -r0 h0 300
   vertex -r0 h0 0
   vertex r0 h0 300
  endloop
 endfacet
endsolid nozzel
solid inlet
 facet normal -1 -0 0
  outer loop
   vertex r0 h0 300
   vertex r0 -2000 0
   vertex r0 -2000 300
  endloop
 endfacet
 facet normal -1 0 -0
  outer loop
   vertex r0 h0 300
   vertex r0 h0 0
   vertex r0 -2000 0
  endloop
 endfacet
endsolid inlet
