solid groundAndWall
 facet normal 0 1 0
  outer loop
   vertex 150 0 0
   vertex 0 0 0
   vertex 0 0 3
  endloop
 endfacet
 facet normal 0 1 -0
  outer loop
   vertex 150 0 0
   vertex 0 0 3
   vertex 150 0 3
  endloop
 endfacet
endsolid groundAndWall
solid nozzel
