  outer loop
   vertex -1.5 5 0
   vertex 1.5 5 0
   vertex 1.5 5 3
  endloop
 endfacet
 facet normal 0 -1 0
  outer loop
   vertex -1.5 5 3
   vertex -1.5 5 0
   vertex 1.5 5 3
  endloop
 endfacet
endsolid nozzel
solid inlet
 facet normal -1 -0 0
  outer loop
   vertex 1.5 5 3
   vertex 1.5 -20 0
   vertex 1.5 -20 3
  endloop
 endfacet
 facet normal -1 0 -0
  outer loop
   vertex 1.5 5 3
   vertex 1.5 5 0
   vertex 1.5 -20 0
  endloop
 endfacet
endsolid inlet
