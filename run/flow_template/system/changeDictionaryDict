FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      changeDictionaryDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

boundary
{
    front 
    {
        inGroups           1 (frontAndBack);
    }
    back
    {
        inGroups           1 (frontAndBack);
    }
    axis
    {
        inGroups	    1 (outOrIn);
    }    
    top
    {
        inGroups	    1 (outOrIn);
    }
    side	
    {
        inGroups	    1 (outOrIn);
    }
}

